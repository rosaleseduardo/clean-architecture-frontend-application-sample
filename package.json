{
  "name": "domain-driven-architecture-frontend-application-sample",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test:dev": "vitest --ui",
    "test:prod": "vitest --run",
    "coverage": "vitest run --coverage",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint . --report-unused-disable-directives",
    "commit:changes": "git add -A && cz",
    "pre-commit": "npm run test:prod && npm run lint",
    "install-deps:clean": "pnpm install --frozen-lockfile",
    "install-deps": "pnpm install",
    "start-server": "json-server -p 3500 -w src/entities/counter/infrastructure/data/db.json"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "9.7.0",
    "@testing-library/jest-dom": "6.4.6",
    "@testing-library/react": "16.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.12",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/parser": "7.16.1",
    "@vitejs/plugin-react": "4.3.1",
    "@vitest/ui": "2.0.3",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.x",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-tsdoc": "0.3.0",
    "globals": "15.8.0",
    "husky": "9.1.1",
    "jsdom": "24.1.1",
    "json-server": "1.0.0-beta.1",
    "lint-staged": "15.2.7",
    "typescript": "5.5.3",
    "typescript-eslint": "7.16.1",
    "vite": "5.3.4",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
